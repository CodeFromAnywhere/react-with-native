{
  "name": "react-with-native-form-inputs",
  "version": "1.0.15",
  "main": "build/index.js",
  "source": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "up": "npm version patch -f",
    "pub": "yarn up && npm publish",
    "dev": "npx tsc-watch --onSuccess 'yarn dev:tw'",
    "dev:tw": "tailwindcss -o css.css",
    "minify": "echo build/**/*.js | xargs -n1 sh -c 'terser --output build/${1#build/} --compress --mangle -- $1' argv0",
    "minify2": "bash -c \"for f in build/*.js; do terser $f --output $f --compress --mangle; done\""
  },
  "license": "ISC",
  "dependencies": {
    "react-with-native-ui": "*"
  },
  "devDependencies": {
    "@react-native-community/datetimepicker": "^6.1.3",
    "@types/react": "17.0.30",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-datepicker": "^4.4.1",
    "@types/react-dom": "^17.0.11",
    "@types/react-native": "^0.66.9",
    "@types/react-stars": "^2.2.1",
    "rc-time-picker": "^3.7.3",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-datepicker": "^4.8.0",
    "react-datetime": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.1",
    "react-map-gl": "^7.0.13",
    "react-native": "*",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-stars": "^2.2.5",
    "react-with-native": "*",
    "tailwindcss": "^3.0.24",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react-with-native": "*"
  }
}
